{
  "name": "morkro.de",
  "version": "1.4.6",
  "description": "Portfolio of Moritz Kröger",
  "homepage": "https://moritz.berlin",
  "author": "Moritz Kröger <write@morkro.de>",
  "scripts": {
    "lint:js": "eslint --ignore-path=.eslintignore scripts/",
    "lint:css": "stylelint \"_sass/**/*.scss\" --syntax scss",
    "lint": "npm run lint:js && npm run lint:css",
    "rollup": "rollup --config",
    "watch": "watch -p \"scripts/*.js\" -c \"npm run rollup\"",
    "start": "npm run lint && npm run js && jekyll serve --watch",
    "dev": "npm run lint && npm run rollup && bundle exec jekyll serve",
    "prod": "npm run lint && npm run rollup && JEKYLL_ENV=PROD jekyll build",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^6.0.0",
    "eslint": "^6.7.2",
    "rollup": "^1.27.13",
    "rollup-plugin-babel-minify": "^9.1.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "snyk": "^1.260.0",
    "stylelint": "^12.0.0",
    "stylelint-scss": "^3.13.0"
  },
  "dependencies": {
    "fontfaceobserver": "^2.1.0",
    "whatwg-fetch": "^3.0.0"
  },
  "snyk": true
}
