{
  "name": "morkro.de",
  "version": "1.6.5",
  "description": "Portfolio of Moritz Kröger",
  "homepage": "https://moritz.berlin",
  "author": "Moritz Kröger <write@morkro.de>",
  "scripts": {
    "lint:js": "eslint src/scripts/",
    "lint:css": "stylelint \"src/_sass/**/*.scss\"",
    "lint": "npm run lint:js && npm run lint:css",
    "start": "npm run lint && npm run rollup && jekyll serve --watch",
    "dev": "npm run lint && npm run rollup && bundle exec jekyll serve --livereload",
    "prod": "npm run lint && npm run rollup && NODE_ENV=PROD jekyll build",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "devDependencies": {
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
    "eslint": "^8.2.0",
    "postcss-scss": "^4.0.2",
    "snyk": "^1.760.0",
    "stylelint": "^14.0.1",
    "stylelint-scss": "^4.0.0"
  },
  "dependencies": {
    "fontfaceobserver": "^2.1.0",
    "html-minifier": "^4.0.0",
    "sass": "^1.62.1"
  },
  "snyk": true
}
